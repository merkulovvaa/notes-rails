<%= form_with(model: @note, html: { multipart: true }, local: true) do |f| %>
  <p><%= f.label :title %></p>
  <p><%= f.text_field :title %></p>

  <p><%= f.label :text %></p>
  <p><%= f.text_field :text %></p>

  <p><%= f.label :user_id %></p>
  <p><%= f.collection_select :user_id, User.all, :id, :full_name %></p>

  <p><%= f.submit %></p>
<% end %>